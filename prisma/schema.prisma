generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Client {
  id          Int      @id @default(autoincrement())
  firstName   String
  lastName    String
  address     String
  dob         DateTime
  email       String   @unique
  cell        String
  companyName String
  price       String
  comments    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
